{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "feedless-config": "npx tsc feedless-config-writer.ts --outDir build/ && node build/feedless-config-writer.js > feedless-config.json",
    "prebuild": "yarn graphql-codegen",
    "prestart:feedless": "yarn graphql-codegen && sh prepare-product.sh feedless false",
    "start:feedless": "ng serve",
    "prestart:rss-proxy": "yarn graphql-codegen && sh prepare-product.sh rssProxy false",
    "start:rss-proxy": "ng serve",
    "prestart:page-change-tracker": "yarn graphql-codegen && sh prepare-product.sh pageChangeTracker false",
    "start:page-change-tracker": "ng serve",
    "prestart:untold-notes": "yarn graphql-codegen && sh prepare-product.sh untoldNotes true",
    "start:untold-notes": "ng serve",
    "prestart:reader": "yarn graphql-codegen && sh prepare-product.sh reader false",
    "start:reader": "ng serve",
    "prestart:upcoming": "yarn graphql-codegen && sh prepare-product.sh upcoming false",
    "start:upcoming": "ng serve",
    "start:upcoming:prod": "ng serve -c production",
    "prestart:visual-diff": "yarn graphql-codegen && sh prepare-product.sh visualDiff false",
    "start:visual-diff": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c production",
    "test": "ng test",
    "test:ci": "ng test -c ci",
    "lint": "yarn prettier --write --log-level=warn .",
    "e2e": "ng e2e",
    "outdated": "upgrade-interactive --latest",
    "codegen": "graphql-codegen --config codegen.yml && yarn feedless-config"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/common": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@angular/service-worker": "17.3.12",
    "@apollo/client": "3.11.1",
    "@codemirror/autocomplete": "^6.12.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-css": "^6.2.1",

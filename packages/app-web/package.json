{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prebuild": "yarn graphql-codegen",
    "prestart:feedless": "yarn graphql-codegen",
    "start:feedless": "ng serve",
    "prestart:rss-builder": "yarn graphql-codegen && sh prepare-product.sh rssBuilder",
    "start:rss-builder": "ng serve",
    "prestart:page-change-tracker": "yarn graphql-codegen && sh prepare-product.sh pageChangeTracker",
    "start:page-change-tracker": "ng serve",
    "prestart:untold-notes": "yarn graphql-codegen && sh prepare-product.sh untoldNotes",
    "start:untold-notes": "ng serve",
    "prestart:reader": "yarn graphql-codegen && sh prepare-product.sh reader",
    "start:reader": "ng serve",
    "prestart:upcoming": "yarn graphql-codegen && sh prepare-product.sh upcoming",
    "start:upcoming": "ng serve",
    "prestart:visual-diff": "yarn graphql-codegen && sh prepare-product.sh visualDiff",
    "start:visual-diff": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c production",
    "test": "#ng test",
    "test:ci": "#ng test -c ci",
    "lint": "yarn prettier --write --loglevel=warn . && ng lint",
    "e2e": "ng e2e",
    "outdated": "upgrade-interactive --latest",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "private": true,
